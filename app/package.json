{
  "name": "app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "prebuild": "pnpm generate-api-types",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "generate-api-types": "openapi-typescript ../openapi.json -o src/lib/api-types.ts 2>/dev/null || openapi-typescript http://localhost:3000/openapi -o src/lib/api-types.ts 2>/dev/null || echo '⚠️  Could not generate API types. Using existing types. Make sure backend is running or openapi.json exists.'"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.90.19",
    "openapi-fetch": "^0.9.3",
    "react": "^19.2.3",
    "react-dom": "^19.2.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^24.10.9",
    "@types/react": "^19.2.9",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "eslint": "^9.39.2",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.26",
    "globals": "^16.5.0",
    "openapi-typescript": "^7.4.3",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.53.1",
    "vite": "^7.3.1"
  }
}
